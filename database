CREATE TABLE  "ASSETS" 
   (	"ASSET_SERIAL_NUMBER" VARCHAR2(16) COLLATE "USING_NLS_COMP" NOT NULL ENABLE, 
	"ASSET_NAME" VARCHAR2(32) COLLATE "USING_NLS_COMP" NOT NULL ENABLE, 
	"ASSET_LOCATION" VARCHAR2(32) COLLATE "USING_NLS_COMP" NOT NULL ENABLE, 
	"ASSET_MODEL" VARCHAR2(16) COLLATE "USING_NLS_COMP", 
	"ASSET_MANUFACTURER" VARCHAR2(16) COLLATE "USING_NLS_COMP", 
	"ASSET_YEAR" VARCHAR2(4) COLLATE "USING_NLS_COMP", 
	"TEAMS_IN_CHARGE" VARCHAR2(16) COLLATE "USING_NLS_COMP", 
	"ASSET_TYPE" VARCHAR2(16) COLLATE "USING_NLS_COMP", 
	"ASSET_VENDOR" VARCHAR2(16) COLLATE "USING_NLS_COMP", 
	"ASSET_LONG" NUMBER, 
	"ASSET_LAT" NUMBER, 
	 CONSTRAINT "ASSETS_PK" PRIMARY KEY ("ASSET_SERIAL_NUMBER")
  USING INDEX  ENABLE
   )  DEFAULT COLLATION "USING_NLS_COMP"
/
