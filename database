CREATE OR REPLACE EDITIONABLE TRIGGER  "AOP_CONFIG_BIU" 
    before insert or update  
    on aop_config 
    for each row 
begin 
    if :new.id is null then 
        :new.id := 1; 
    end if; 
    if inserting then 
        :new.created := sysdate; 
        :new.created_by := nvl(sys_context('APEX$SESSION','APP_USER'),user); 
    end if; 
    :new.updated := sysdate; 
    :new.updated_by := nvl(sys_context('APEX$SESSION','APP_USER'),user); 
end aop_config_biu; 
 

/
ALTER TRIGGER  "AOP_CONFIG_BIU" ENABLE
/
CREATE OR REPLACE EDITIONABLE TRIGGER  "AOP_SAMPLE_CUSTOMERS_BIU" 
  before insert or update ON AOP_SAMPLE_customers FOR EACH ROW 
DECLARE 
  cust_id number; 
BEGIN 
  if inserting then   
    if :new.customer_id is null then 
      select AOP_SAMPLE_cust_seq.nextval 
        into cust_id 
        from dual; 
      :new.customer_id := cust_id; 
    end if; 
  end if; 
END; 
 

/
ALTER TRIGGER  "AOP_SAMPLE_CUSTOMERS_BIU" ENABLE
/
CREATE OR REPLACE EDITIONABLE TRIGGER  "AOP_SAMPLE_ORDERS_BIU" 
  before insert or update ON AOP_SAMPLE_orders FOR EACH ROW 
DECLARE 
  order_id number; 
BEGIN 
  if inserting then   
    if :new.order_id is null then 
      select AOP_SAMPLE_ord_seq.nextval 
        INTO order_id 
        FROM dual; 
      :new.order_id := order_id; 
    end if; 
  end if;   
END; 
 

/
ALTER TRIGGER  "AOP_SAMPLE_ORDERS_BIU" ENABLE
/
CREATE OR REPLACE EDITIONABLE TRIGGER  "AOP_SAMPLE_ORDER_ITEMS_AIUD" 
  after insert or update or delete on AOP_SAMPLE_order_items 
begin 
  -- Update the Order Total when any order item is changed 
  update AOP_SAMPLE_orders set order_total = 
  (select sum(unit_price*quantity) from AOP_SAMPLE_order_items 
    where AOP_SAMPLE_order_items.order_id = AOP_SAMPLE_orders.order_id); 
end; 
 

/
ALTER TRIGGER  "AOP_SAMPLE_ORDER_ITEMS_AIUD" ENABLE
/
CREATE OR REPLACE EDITIONABLE TRIGGER  "AOP_SAMPLE_ORDER_ITEMS_BI" 
  BEFORE insert on "AOP_SAMPLE_ORDER_ITEMS" for each row 
declare 
  order_item_id number; 
begin 
  if :new.order_item_id is null then 
    select AOP_SAMPLE_order_items_seq.nextval  
      into order_item_id  
      from dual; 
    :new.order_item_id := order_item_id; 
  end if; 
end; 
 

/
ALTER TRIGGER  "AOP_SAMPLE_ORDER_ITEMS_BI" ENABLE
/
CREATE OR REPLACE EDITIONABLE TRIGGER  "AOP_SAMPLE_ORDER_ITEMS_BIU" 
  before insert or update on AOP_SAMPLE_order_items for each row 
declare 
  l_list_price number; 
begin 
  if :new.unit_price is null then 
    -- First, we need to get the current list price of the order line item 
    select list_price 
    into l_list_price 
    from AOP_SAMPLE_product_info 
    where product_id = :new.product_id; 
    -- Once we have the correct price, we will update the order line with the correct price 
    :new.unit_price := l_list_price; 
  end if; 
end; 
 

/
ALTER TRIGGER  "AOP_SAMPLE_ORDER_ITEMS_BIU" ENABLE
/
CREATE OR REPLACE EDITIONABLE TRIGGER  "AOP_SAMPLE_PRODUCT_INFO_BIU" 
  before insert or update ON AOP_SAMPLE_product_info FOR EACH ROW 
DECLARE 
  prod_id number; 
BEGIN 
  if inserting then   
    if :new.product_id is null then 
      select AOP_SAMPLE_prod_seq.nextval 
        into prod_id 
        from dual; 
      :new.product_id := prod_id; 
    end if; 
  end if; 
END; 
 

/
ALTER TRIGGER  "AOP_SAMPLE_PRODUCT_INFO_BIU" ENABLE
/
CREATE OR REPLACE EDITIONABLE TRIGGER  "BI_AOP_PDF_FORM" 
  before insert on "AOP_PDF_FORM"                
  for each row   
begin    
  if :NEW."ID" is null then  
    select "AOP_PDF_FORM_SEQ".nextval into :NEW."ID" from sys.dual;  
  end if;  
end; 
 

/
ALTER TRIGGER  "BI_AOP_PDF_FORM" ENABLE
/
CREATE OR REPLACE EDITIONABLE TRIGGER  "BI_ASSETS" 
  before insert on "ASSETS"                
  for each row   
begin    
  if :NEW."ASSET_SERIAL_NUMBER" is null then  
    select "ASSETS_SEQ".nextval into :NEW."ASSET_SERIAL_NUMBER" from sys.dual;  
  end if;  
end;  
 

/
ALTER TRIGGER  "BI_ASSETS" ENABLE
/
