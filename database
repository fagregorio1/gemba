CREATE TABLE  "AOP_SAMPLE_CUSTOMERS" 
   (	"CUSTOMER_ID" NUMBER NOT NULL ENABLE, 
	"CUST_FIRST_NAME" VARCHAR2(20) COLLATE "USING_NLS_COMP" NOT NULL ENABLE, 
	"CUST_LAST_NAME" VARCHAR2(20) COLLATE "USING_NLS_COMP" NOT NULL ENABLE, 
	"CUST_STREET_ADDRESS1" VARCHAR2(60) COLLATE "USING_NLS_COMP", 
	"CUST_STREET_ADDRESS2" VARCHAR2(60) COLLATE "USING_NLS_COMP", 
	"CUST_CITY" VARCHAR2(30) COLLATE "USING_NLS_COMP", 
	"CUST_STATE" VARCHAR2(2) COLLATE "USING_NLS_COMP", 
	"CUST_POSTAL_CODE" VARCHAR2(10) COLLATE "USING_NLS_COMP", 
	"CUST_EMAIL" VARCHAR2(30) COLLATE "USING_NLS_COMP", 
	"PHONE_NUMBER1" VARCHAR2(25) COLLATE "USING_NLS_COMP", 
	"PHONE_NUMBER2" VARCHAR2(25) COLLATE "USING_NLS_COMP", 
	"URL" VARCHAR2(100) COLLATE "USING_NLS_COMP", 
	"CREDIT_LIMIT" NUMBER(9,2), 
	"TAGS" VARCHAR2(4000) COLLATE "USING_NLS_COMP", 
	 CONSTRAINT "AOP_SAMPLE_CUST_CREDIT_MAX" CHECK (credit_limit <= 5000) ENABLE, 
	 CONSTRAINT "AOP_SAMPLE_CUSTOMERS_PK" PRIMARY KEY ("CUSTOMER_ID")
  USING INDEX  ENABLE, 
	 CONSTRAINT "AOP_SAMPLE_CUSTOMERS_UK" UNIQUE ("CUST_FIRST_NAME", "CUST_LAST_NAME")
  USING INDEX  ENABLE
   )  DEFAULT COLLATION "USING_NLS_COMP"
/
CREATE TABLE  "AOP_SAMPLE_ORDERS" 
   (	"ORDER_ID" NUMBER NOT NULL ENABLE, 
	"CUSTOMER_ID" NUMBER NOT NULL ENABLE, 
	"ORDER_TOTAL" NUMBER(8,2), 
	"ORDER_TIMESTAMP" TIMESTAMP (6) WITH LOCAL TIME ZONE, 
	"USER_NAME" VARCHAR2(100) COLLATE "USING_NLS_COMP", 
	"TAGS" VARCHAR2(4000) COLLATE "USING_NLS_COMP", 
	 CONSTRAINT "AOP_SAMPLE_ORDER_TOTAL_MIN" CHECK (order_total >= 0) ENABLE, 
	 CONSTRAINT "AOP_SAMPLE_ORDER_PK" PRIMARY KEY ("ORDER_ID")
  USING INDEX  ENABLE
   )  DEFAULT COLLATION "USING_NLS_COMP"
/
